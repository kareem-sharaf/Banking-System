{
  "dashboard": {
    "id": null,
    "title": "Spring Boot Banking System",
    "tags": ["spring-boot", "banking"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "JVM Memory Used",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{application=\"banking-system\"}",
            "legendFormat": "{{area}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HTTP Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "http_server_requests_seconds_count{application=\"banking-system\"}",
            "legendFormat": "{{method}} {{uri}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "System CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "system_cpu_usage{application=\"banking-system\"}",
            "legendFormat": "System CPU Usage",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "hikaricp_connections_active{application=\"banking-system\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "hikaricp_connections_idle{application=\"banking-system\"}",
            "legendFormat": "Idle Connections",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "links": []
  }
